{
  "title": "Gulp Static Seed by Aparecium Labs",
  "name": "gulp-static-seed",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "A Gulp starter kit for HTML5 static website development with a congenial build process",
  "homepage": "https://github.com/apareciumlabs/gulp-static-seed#README.md",
  "bugs": {
    "url": "https://github.com/apareciumlabs/gulp-static-seed/issues",
    "email": "opensource@apareciumalabs.com"
  },
  "license": "MIT",
  "author": "Brion Mario",
  "contributors": [
    {
      "name": "Brion Mario",
      "email": "brion@apareciumlabs.com",
      "url": "https://github.com/brionmario"
    }
  ],
  "engines": {
    "node": ">=8.9.1 <9.0.0",
    "yarn": ">=1.3.2 <2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apareciumlabs/gulp-static-seed"
  },
  "keywords": [
    "gulp",
    "css",
    "sass",
    "html",
    "responsive",
    "apareciumlabs",
    "seed"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "scripts": {
    "gulp": "gulp",
    "build:dev": "npm run clean:temp && npm run gulp build --env=development",
    "build:prod": "npm run clean:dist && npm run gulp build --env=production",
    "build": "npm run build:prod",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "clean:temp": "npm run gulp clean --env=development",
    "clean:dist": "npm run gulp clean --env=production",
    "clean": "npm run gulp clean --env=all",
    "gulp-dev-server": "npm run gulp browserSync --base=.temp",
    "lint:scripts": "npm run gulp scripts:lint; exit 0",
    "lint:styles": "npm run gulp styles:lint",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "semver": "npm version $(semver $npm_package_version -i $release --preid $preid)",
    "server:dev": "npm run build:dev && npm run gulp serve --env=development",
    "server:prod": "npm run build:prod && http-server dist -a 192.168.8.101 -o",
    "server": "npm run server:dev",
    "start": "npm run server",
    "test": "jest --testPathIgnorePatterns ./src/vendor && codecov",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "watch:dev": "npm run gulp watch",
    "watch": "npm run watch:dev",
    "postinstall": "bower install"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "all-contributors-cli": "^5.4.0",
    "browser-sync": "2.24.5",
    "browserify": "^16.2.2",
    "codecov": "^3.1.0",
    "del": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-css-url-adjuster": "^0.2.3",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "2.0.5",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-inject": "^4.3.2",
    "gulp-rename": "1.3.0",
    "gulp-sass": "4.0.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "http-server": "^0.11.1",
    "jest": "^23.5.0",
    "main-bower-files": "^2.13.1",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "semver": "^5.5.1"
  }
}
